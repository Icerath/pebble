fn main() {
    let iters = 10000;
    let total_iters = iters * iters;

    let sum = 0 + 0 + 0 + 0 + 0 + 0 + 0 + 0;
    println("${sum}");
    let i = 0;
    while i < iters {
        i += 1;
        let j = 0;
        while j < iters {
            j += 1;
            sum += 1;
        }
    }

    println("${sum}");
    assert sum == total_iters;
}