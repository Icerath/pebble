fn main() {
    let array = [0; 8]
    assert (&array)[0] == 0;
    assert len(&array) == 8;

    for i in 0..8 {
        for j in 0..i {
            array[i] += 1;
        }
    }
    for i in 0..8 {
        assert array[i] == i;
    }

    let arr2d: [[int]] = [[1]];
    assert arr2d[0][0] == 1;
    arr2d[0][0] = 2;
    assert arr2d[0][0] == 2;

    let grow = [];
    assert len(&grow) == 0;
    push(&grow, 1);
    assert len(&grow) == 1;
    assert grow[0] == 1;
    assert pop(&grow) == 1;
    assert len(&grow) == 0;
}
