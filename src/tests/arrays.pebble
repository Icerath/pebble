fn main() {
    let array = [0; 8]
    let i = 0
    while i < 8 {
        let j = 0
        while (j < i) {
            array[i] += 1
            j += 1
        }
        i += 1
    }
    i = 0;
    while (i < 8) {
        if (array[i] != i) {
            abort
        }
        i += 1;
    }

    let arr2d: [[int]] = [[1]];
    if arr2d[0][0] != 1 { abort }
    arr2d[0][0] = 2;
    if arr2d[0][0] != 2 { abort }
}
